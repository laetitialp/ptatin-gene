#PETSc Option Table entries:
-A11_operator_type 0,1,1,1,1
-coefficient_projection_type -1
-checkpoint_every 1
-dau_nlevels 2
-dt_max 0.05
#-constant_dt 0.05
-fieldsplit_p_ksp_type preonly
-fieldsplit_p_pc_type jacobi

-fieldsplit_u_ksp_max_it 10000
-fieldsplit_u_ksp_monitor
-fieldsplit_u_ksp_rtol 1.0e-2
-fieldsplit_u_ksp_type fgmres

-fieldsplit_u_mg_coarse_ksp_max_it 1000
#-fieldsplit_u_mg_coarse_ksp_monitor_true_residual
-fieldsplit_u_mg_coarse_ksp_rtol 1.0e-2
-fieldsplit_u_mg_coarse_ksp_type cg
-fieldsplit_u_mg_coarse_pc_type  asm
-fieldsplit_u_mg_coarse_pc_asm_overlap 3
 
-fieldsplit_u_mg_levels_est_ksp_norm_type NONE
-fieldsplit_u_mg_levels_ksp_chebychev_estimate_eigenvalues 0,0.2,0,1.1
-fieldsplit_u_mg_levels_ksp_max_it 20
-fieldsplit_u_mg_levels_ksp_norm_type NONE
-fieldsplit_u_mg_levels_ksp_type chebychev
-fieldsplit_u_mg_levels_pc_type jacobi
-ksp_atol 1.0e-6

## MORE ROBUST ON DEFORMED MESHES? 
-fieldsplit_u_mg_levels_ksp_type cg

#-ksp_monitor_true_residual
-ksp_type fgmres
-log_summary
-model_multilayer_folding_Lx 1000
-model_multilayer_folding_Lz 500
-model_multilayer_folding_amp_factor 0.5 #from the layer thickness

# Three layer model implies we need four interfaces
-model_multilayer_folding_n_interfaces       4
-model_multilayer_folding_interface_heights  0.0,50.0,60,100.0
# Three layer model implies we need 3 element resolutions and 3 viscosities and 3 densities
-model_multilayer_folding_layer_eta     0.0025,1.0,0.0025
-model_multilayer_folding_layer_res_j   2,1,2 
-model_multilayer_folding_layer_rho     2.7,2.7,2.7
-mx 4
-mz 4

-model_multilayer_folding_vx      0.005
-model_multilayer_folding_exx     -0.2
-model_multilayer_folding_bc_type 1                              

# low resolution test
#-mx 10
#-my 10
#-model_multilayer_folding_layer_res_k 2,2,2,2,2,2,2,2

-ptatin_model multilayer_folding
-nsteps 10000
-output_frequency 1
-output_path test_multilayer_ptatin3d
-pc_fieldsplit_schur_factorization_type upper
-pc_fieldsplit_type schur
-pc_type fieldsplit
-snes_type ksponly
-stk_velocity_da_mat_type baij

-stk_velocity_da_refine_hierarchy_x 2,2
-stk_velocity_da_refine_hierarchy_y 2,2
-stk_velocity_da_refine_hierarchy_z 1,1

# dump the geometric multi-grid mesh hierarchy 
#-stk_velocity_da_view_hierarchy

#End of PETSc Option Table entries
