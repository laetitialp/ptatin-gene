-model_ocean_Hst_cutoff_0 400.0e6
-model_ocean_eps_min_0 0.0
-model_ocean_eps_max_0 1.0
-model_ocean_alpha_0 3.0e-5
-model_ocean_beta_0 1.0e-5
-model_ocean_rho_0 3000.0
